{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["es2022"],
		"rootDir": "..",
		"module": "commonjs",
		"paths": {
			"@/*": ["./*"],
			"~/*": ["../*"]
		},
		"resolveJsonModule": true,
		"allowJs": true,
		"strict": true,
		"noImplicitAny": true,
		"outDir": "../build/backend",
		"sourceMap": false,
		"esModuleInterop": true,
		"skipLibCheck": true
	}
}
